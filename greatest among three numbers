#include<stdio.h>
int main() {
int a,b,c;
printf("enter first number:");
scanf("%d", &a);
printf("enter second number");
scanf("%d" , &b);
printf("enter third number");
scanf("%d" , &c);
if(a>b && a>c) {
	printf("a is greater integer");
}
else if(b>a && b>c) {
	printf("b is greater integer");
}
else if(c>b && c>a) {
	printf("c is greater");
}
else {
	printf("thank you");
}
return 0;
}
	
